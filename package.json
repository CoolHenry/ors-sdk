{
    "name": "ors-web-sdk",
    "version": "1.3.7",
    "description": "ORS SDK with multi-platform support",
    "type": "module",
    "scripts": {
        "changeset": "changeset",
        "version": "changeset version",
        "release": "pnpm run build:prod && changeset publish",
        "build": "pnpm -r run build:prod",
        "lint": "pnpm run build && pnpm -r run lint",
        "build:prod": "pnpm run lint && pnpm run copy",
        "copy": "node scripts/copy.js",
        "test": "pnpm -r run test",
        "example:dev": "pnpm -r --parallel --filter \"@ors-sdk/web\" --filter \"example-web\" run dev",
        "prepare": "husky"
    },
    "engines": {
        "node": ">=16.19.0"
    },
    "devDependencies": {
        "@babel/preset-typescript": "^7.27.1",
        "@changesets/cli": "^2.29.5",
        "@commitlint/cli": "^19.8.1",
        "@commitlint/config-conventional": "^19.8.1",
        "@commitlint/types": "^19.8.1",
        "@jest/globals": "^30.0.5",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.17.50",
        "@typescript-eslint/eslint-plugin": "^5.59.8",
        "@typescript-eslint/parser": "^5.59.8",
        "babel-jest": "^30.0.5",
        "eslint": "^8.41.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^9.1.7",
        "jest": "^30.0.5",
        "jest-environment-jsdom": "^30.0.5",
        "lint-staged": "^16.2.0",
        "prettier": "^2.8.8",
        "rollup-plugin-gzip": "^4.1.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.1.3"
    },
    "packageManager": "pnpm@10.11.0"
}